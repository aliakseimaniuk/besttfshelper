{
    "name": "TFS helper",
    "description": "Help with tfs",
    "version": "1.0",
    "permissions": [
      "tabs",  "storage", "http://*/*", "https://*/*"
    ],
     "content_scripts": [
      { 
          "matches": [ "http://*/*", "https://*/*"],
          "js": ["jquery.min.js"]
      }
     ],
     "background": {
         "scripts": ["background.js"],
         "persistent": false
     },
  "options_page": "options.html",
  "page_action": {
      "default_title": "TFS best helper",
      "default_icon": "images/icon.png",
      "default_popup": "popup.html"
  },
  "manifest_version": 2,
  "icons": { "64": "images/icon.png" }
}
